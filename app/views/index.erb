<div class="container">
  <h1>Users</h1>
  <% if current_user %>
  <form action="/review/new">
    <select>
      <% @user.each do |user| %>
      <option value="<%= user.name %>"> <%= user.name %></option>
      <% end %>
    </select>
  </form>
    <ul>
    <% @reviews.each do |review| %>
      <li><a href="/review/<%= review.id %>"> <%= review.title %></a></li>
    <% end %>
    </ul>
    <a href="/review/sent">Review sent feedback</a>
  <% end %>
</div>
